{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/notes/(.*)",
      "destination": "/http/routes/api/notes/index.js"
    },
    {
      "source": "/api/openai/(.*)",
      "destination": "/http/routes/api/openai/index.js"
    },
    {
      "source": "/api/users/(.*)",
      "destination": "/http/routes/api/users/index.js"
    },
    {
      "source": "/api",
      "destination": "/http/routes/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/http/index.js"
    }
  ]
}
